{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../build/electron",
    "baseUrl": "../",
    "paths": {
      "_Global": ["types/global"],
      "_Electron": ["types/electron"],
      "_Music-Metadata": ["types/music-metadata"]
    }
  }
}
