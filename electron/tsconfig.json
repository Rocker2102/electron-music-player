{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../build/electron",
    "typeRoots": [
      "../types",
      "../node_modules/@types"
    ]
  },
  "include": [
    "../electron",
    "../types"
  ],
  "exclude": [
    "../node_modules",
    "../tmp",
    "../dev"
  ]
}
